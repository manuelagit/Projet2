@model List<Projet2.Models.Club>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Recherche club</title>
</head>
<center>
    <body>
        <h1>Recherche club</h1>
        <table border="1" style="margin: 20px">
            <tr>
                <th>Id</th>
                <th>Nom du club</th>
                <th>Adresse</th>
                <th>adresse Email</th>
                <th>Mot de passe</th>
            </tr>
            <tbody>
                @foreach (var member in Model)

                {
                            
                    <tr>
                        <td>@member.Id</td>
                        <td>@member.InfosClub.NomClub</td>
                        <td>@member.InfosClub.Adresse.NumeroRue, @member.InfosClub.Adresse.NomRue - @member.InfosClub.Adresse.CodePostal @member.InfosClub.Adresse.NomVille </td>
                        <td>@member.Compte.AdressEmail</td>
                        <td>@member.Compte.MotDePasse</td>
                    </tr>
                                
                            }
            </tbody>
        </table>

        <div style="margin: 20px">
            <input type="button" onclick="window.location.href='https://localhost:5001/Home';" class="btn btn-warning btn-lg" value="Retour accueil" />
            <button type="submit" onclick="window.location.href='https://localhost:5001/Home/PageClub';">Page du club</button>
        </div>



    </body>
</center>
</html>

