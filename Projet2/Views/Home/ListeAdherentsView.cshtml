@model List<Projet2.Models.Adherent>
@{
    ViewBag.Title = "AdherentList";
    Layout = "_Layout";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Liste des adherents</title>
</head>
<center>
    <body>
        <h1>Liste des adhérents</h1>
        <table border="1" style="margin: 20px">
            <tr>
                <th>Id</th>
                <th>Club</th>
                <th>Compte</th>
                
            </tr>
            <tbody>
                @foreach (var member in Model)

                {
                    <tr>
                        <td>@member.Id</td>
                        <td>@member.Club.InfosClub.NomClub</td>
                        <td>@member.Compte.AdressEmail</td>
                       
                    </tr>
                }
            </tbody>
        </table>

        



    </body>
</center>
</html>
